<?xml version="1.0"?>
<!DOCTYPE definitions [
  <!ENTITY build.targets SYSTEM "/usr/local/mgi/etc/build.lib.targets.xml">
  <!-- uncomment this entry if you are using a local targets file in addition to the etc one
  <!ENTITY local.build.targets SYSTEM "build.targets.xml">
  -->
]>

<!-- Enter the name of your current project here 
 -->
<project name="lib_java_core" default="compile" basedir=".">

  <!-- Enter the name of your jar file here minus the dot jar suffix
   -->
  <property name="jar.name" value="core"/>

  <!-- To override property definitions in the following 
       properties file, add new definitions in preceding lines.
       The first definition of a property takes precedence 
   -->
  <property file="build.properties"/>
  <property file="/usr/local/mgi/etc/build.lib.properties"/>

  <!-- Universal targets get included here  
   -->
  &build.targets;
  <!-- uncomment this entry if you are using a local targets file in addition to the etc one
  &local.build.targets;
  -->

</project>

